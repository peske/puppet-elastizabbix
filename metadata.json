{
  "name": "fatdragon-elastizabbix",
  "version": "0.1.2", 
  "author": "Fat Dragon", 
  "license": "Apache-2.0",
  "summary": "Installs and configures elastizabbix - agent and template for monitoring ElasticSearch cluster in Zabbix.",
  "source": "https://github.com/peske/puppet-elastizabbix",
  "project_page": "https://www.fatdragon.me/blog/2016/06/puppet-zabbix-and-elasticsearch-elastizabbix-module",
